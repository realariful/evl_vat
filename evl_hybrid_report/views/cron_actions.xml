<?xml version="1.0" ?>
<odoo>
    <data noupdate="0">

        <record id="generate_report" model="ir.cron">
            <field name="name">Generate Report</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
